msgid ""
"\n"
"<emphasis>C</emphasis> - character string\n"
msgstr ""

#: template/./doc/sql.sgml:1780
msgid ""
"\n"
"<emphasis>N</emphasis> - numeric\n"
msgstr ""

#: template/./doc/sql.sgml:1783
msgid ""
"\n"
"<emphasis>D</emphasis> - date\n"
msgstr ""

#: template/./doc/sql.sgml:1786
msgid ""
"\n"
"<emphasis>T</emphasis> - datetime\n"
msgstr ""

#: template/./doc/sql.sgml:1789
